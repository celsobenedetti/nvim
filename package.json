{
  "name": "celso-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": [
          "go"
        ],
        "path": "./snippets/go.json"
      },
      {
        "language": [
          "lua"
        ],
        "path": "./snippets/lua.json"
      },
      {
        "language": [
          "markdown"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "org",
          "orgmode"
        ],
        "path": "./snippets/org.json"
      }
    ]
  }
}
