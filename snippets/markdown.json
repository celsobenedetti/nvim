{
  "Insert code block": {
    "prefix": "cb",
    "body": ["```${1:language}", "$0", "```"],
    "description": "Insert fenced code block"
  },
  "underline": {
    "description": "underline",
    "prefix": "u",
    "body": ["<u>$0</u>"]
  },
  "quote quote block": {
    "description": "quote quote block",
    "prefix": "q",
    "body": ["> [!QUOTE]", "> $0"]
  },
  " quote block": {
    "description": "quote block",
    "prefix": "qb",
    "body": ["> [!$1]", "> $2"]
  }
